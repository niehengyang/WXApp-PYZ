/**early_warning.wxss**/
page{
  background: #1A203B;
  display: flex;
  justify-content: center;
}
.weui-navbar {
    height: 88rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    z-index: 500;
    top: 0;
    width: 100%;
    background: #1A203B;
    align-items: center;
}
.weui-navbar__item {
    width: 350rpx;
    height: 88rpx;
    position: relative;
    display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    text-align: center;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    opacity: 0.6;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 2rpx;
    flex-direction: column;
    align-items: center;
    border-bottom:4rpx solid;
    margin: 2rpx; 
}
.weui-navbar__item.weui-bar__item_on {
    color: #1185E4; 
    border-bottom-color: #00C6FB;
    opacity: unset;
}


.weui-navbar__item:after {
    content: " ";
    position: absolute;
    right: 0;
    top: 0;
    width: 2rpx;
    bottom: 0;
    border-right: 8rpx solid none;
    color: #CCCCCC;
    -webkit-transform-origin: 100% 0;
    transform-origin: 100% 0;
    -webkit-transform: scaleX(0.5);
    transform: scaleX(0.5);
}
.weui-navbar:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1rpx;
    border-bottom: 1rpx solid #CCCCCC;
    color: #CCCCCC;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.weui-navbar .weui-tab__panel {
    padding-top: 50px;
    padding-bottom: 0;
}
.weui-tab__panel {
    box-sizing: border-box;
    height: 100%;
    padding-bottom: 50px;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    display: flex;
    justify-content: center;
}
.already-processed_box{
  width: 690rpx;
  height: 1062rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  margin-top: 10rpx;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-content: center;
}
.processed_item{
  width: 656rpx;
  height: 130rpx;
  margin-top: 20rpx;
  border-bottom: 2rpx solid;
  border-bottom-color:  #F0F0F4;
  display: flex;
  flex-direction: row;

}
.red_icon{
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background: #CC0000;
  margin: 14rpx;
}
.device_item{
  align-items: center;
}
.device_label{
  width: 600rpx;
  height: 44rpx;
  display: flex;
  align-content: space-between;
  align-items: center;
  justify-content: space-between;
}
.devie_name{
font-family: PingFangSC-Regular;
font-size: 16px;
color: #1A203B;
letter-spacing: 0.12px;
}
.device_style{
font-family: PingFangSC-Regular;
font-size: 16px;
color: #1A203B;
letter-spacing: 0.12px;
}
.time_label{
font-family: PingFangSC-Regular;
font-size: 12px;
color: #8E98B4;
