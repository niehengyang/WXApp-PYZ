/**real_time_monitoring.wxss**/
page{
  background: #1A203B;
  display: flex;
  align-items: center;
  align-content: center;
  flex-direction: column;

}
.container{
  width: 100%;
  height: 1300rpx;
  display: flex;
  align-items: center;
  align-content: center;
  justify-content: center;
  flex-direction: column;
  padding: unset;
}
.video_box{
  width: 690rpx;
  height: 388rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column-reverse;
}
.video_item{
  width: 690rpx;
  height: 388rpx;
  display: flex;
  border-radius: 4px;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: absolute;
}
.face_svg{
  width: 690rpx;
  height: 388rpx;
  
}
.video_name{
  width: 690rpx;
  height: 72rpx;
  font-family: PingFangSC-Regular;
font-size: 16px;
color: #FFFFFF;
letter-spacing: -0.1px;
opacity: 0.6;
background: #000000;
border-radius: 0 0 4px 4px;
display: flex;
align-items: center;
text-indent: 20rpx;
}
